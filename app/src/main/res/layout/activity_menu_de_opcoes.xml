<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CCCCCC"
    tools:context=".menus.MenuDeOpcoes">

    <ImageView
        android:id="@+id/pinhatec_opcoes"
        android:layout_width="331dp"
        android:layout_height="143dp"
        android:layout_marginTop="30dp"
        android:background="#CCCCCC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rodape_menu_opcoes"
        tools:ignore="ContentDescription"
        tools:srcCompat="@drawable/rodape_menu_opcoes" />

    <ImageButton
        android:id="@+id/Bt_opc_cadastro"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="botao de cadastro"
        android:scaleType="centerCrop"
        android:src="@drawable/btscadastro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pinhatec_opcoes"
        tools:ignore="HardcodedText"
        tools:src="@drawable/btscadastro" />

    <ImageButton
        android:id="@+id/bt_opc_benef"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="botao de beneficios"
        android:scaleType="centerCrop"
        android:src="@drawable/btsbenef"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Bt_opc_cadastro"
        tools:ignore="HardcodedText"
        tools:src="@drawable/btsbenef" />

    <ImageButton
        android:id="@+id/bt_opc_ponto"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="botao de ponto"
        android:scaleType="centerCrop"
        android:src="@drawable/btsopcponto"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_opc_benef"
        tools:ignore="HardcodedText"
        tools:src="@drawable/btsopcponto" />

    <ImageButton
        android:id="@+id/bt_opc_rendimentos"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="35dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="botao de rendimentos"
        android:scaleType="centerCrop"
        android:src="@drawable/btsrendimento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Bt_opc_cadastro"
        app:layout_constraintTop_toBottomOf="@+id/pinhatec_opcoes"
        tools:ignore="HardcodedText"
        tools:src="@drawable/btsrendimento" />

    <ImageButton
        android:id="@+id/bt_opc_ferias"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="35dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="botao de ferias"
        android:scaleType="centerCrop"
        android:src="@drawable/btsferias"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bt_opc_benef"
        app:layout_constraintTop_toBottomOf="@+id/bt_opc_rendimentos"
        tools:ignore="HardcodedText"
        tools:src="@drawable/btsferias" />

    <ImageButton
        android:id="@+id/bthome"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Home"
        android:scaleType="centerCrop"
        android:src="@drawable/bthouse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/bthouse"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/btrendimentos"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Historico"
        android:scaleType="centerCrop"
        android:src="@drawable/bthistoricos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bthome"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/bthistoricos"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/btopc"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Opcoes"
        android:scaleType="centerCrop"
        android:src="@drawable/btmenuopc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btrendimentos"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.487"
        tools:src="@drawable/btmenuopc"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/btajuda"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Ajuda"
        android:scaleType="centerCrop"
        android:src="@drawable/btmenuajuda"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btopc"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.482"
        tools:src="@drawable/btmenuajuda"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>