<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#cccccc"
    tools:context=".TelasPrincipais.HistoricoPagamentos">

    <ImageView
        android:id="@+id/RdpHistorico"
        android:layout_width="331dp"
        android:layout_height="143dp"
        android:layout_marginTop="30dp"
        android:contentDescription="Rodape de Historico de Pagamentos"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rodape_historico"
        tools:ignore="HardcodedText"
        tools:srcCompat="@drawable/rodape_historico" />

    <ImageButton
        android:id="@+id/bthome"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Home"
        android:scaleType="centerCrop"
        android:src="@drawable/bthouse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/bthouse"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/btrendimentos"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Historico"
        android:scaleType="centerCrop"
        android:src="@drawable/bthistoricos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bthome"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/bthistoricos"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/btopc"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Opcoes"
        android:scaleType="centerCrop"
        android:src="@drawable/btmenuopc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btrendimentos"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.487"
        tools:src="@drawable/btmenuopc"
        tools:ignore="HardcodedText" />

    <ImageButton
        android:id="@+id/btajuda"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="640dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="#CCCCCC"
        android:contentDescription="Ajuda"
        android:scaleType="centerCrop"
        android:src="@drawable/btmenuajuda"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btopc"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.482"
        tools:src="@drawable/btmenuajuda"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Pagamento03"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/custom_editbox"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RdpHistorico"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Pagamento04"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_editbox"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pagamento03"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Pagamento05"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_editbox"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pagamento04"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Pagamento06"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_editbox"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pagamento05"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Pagamento07"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_editbox"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pagamento06"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/Pagamento08"
        android:layout_width="322dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_editbox"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Pagamento07"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>